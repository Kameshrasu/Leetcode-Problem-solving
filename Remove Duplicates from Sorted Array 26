class Solution {
    public int removeDuplicates(int[] nums) {
        int right = nums.length;
        int count=1;
        for(int i=1;i<right;i++){
            if(nums[i]!=nums[i-1]){
                nums[count]=nums[i];
                count++;
            }
        }
    

     
     return count;

    }

    }
